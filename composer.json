{
    "name": "poolzoom/argo",
    "description": "A shipping utility.",
    "keywords": ["argo", "shipping", "tracking number", "php"],
    "type": "library",
    "homepage": "https://github.com/poolzoom/argo",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Sposito",
            "homepage": "https://github.com/dsposito"
        }
    ],
    "require": {
